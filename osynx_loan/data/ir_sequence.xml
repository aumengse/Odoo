<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_member_account" model="ir.sequence">
            <field name="name">Member Account</field>
            <field name="code">member.account.sequence</field>
            <field name="prefix">ACCT%(year)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="3" name="padding"/>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_loan_account" model="ir.sequence">
            <field name="name">Member Account</field>
            <field name="code">loan.account.sequence</field>
            <field name="prefix">LA%(year)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="3" name="padding"/>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_loan_account_payment" model="ir.sequence">
            <field name="name">Member Account</field>
            <field name="code">loan.account.payment.sequence</field>
            <field name="prefix">PAY%(year)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="3" name="padding"/>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>